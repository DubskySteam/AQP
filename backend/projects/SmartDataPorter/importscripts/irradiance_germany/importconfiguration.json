{
    "importer": "FilesImporter",
    "parser": "CSVParser",
    "sourcedir": "/var/home/irradiance_germany/data",
    "workingdir": "/var/home/irradiance_germany/data/working",
    "archivedir": "/var/home/irradiance_germany/data/archiv",
    "inspectiondir": "/var/home/irradiance_germany/data/inspection",
    "server": "http://127.0.0.1:8080/",
    "smartdata": "SmartDataEnvironmental",
    "storage": "public",
    "collection": "irradiation_germany",
    "csv.format": "EXCEL",
    "csv.delimiter": ",",
    "csv.headers": [
        "lat",
        "lon",
        "time",
        "SIS",
        "SID"
    ],
    "csv.mapping": [
        {
            "csv_column": "lat",
            "db_column": "lat",
            "type": "double"
        },
        {
            "csv_column": "lon",
            "db_column": "lon",
            "type": "double"
        },
        {
            "csv_column": "time",
            "db_column": "time",
            "type": "string"
        },
        {
            "csv_column": "SIS",
            "db_column": "global",
            "type": "double"
        },
        {
            "csv_column": "SID",
            "db_column": "direct",
            "type": "double"
        }
    ],
    "flatten": false,
    "geo.transform": true,
    "geo.latAttr": "lat",
    "geo.lonAttr": "lon",
    "geo.srid": "4326",
    "geo.dbattr": "location"
}